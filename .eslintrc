{
  "parserOptions": {
    "ecmaVersion": 2020,
    "impliedStrict": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "jsu": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "prefer-const": "error",
    "no-var": "error",
    "no-multi-spaces": "warn",
    "no-useless-escape": "off",
    "no-unused-vars": "off",
    "no-console": "error",
    "func-call-spacing": ["error", "never"],
    "keyword-spacing": ["warn", { "before": true }],
    "comma-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "dot-notation": [
      "error",
      {
        "allowKeywords": false
      }
    ],
    "semi": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "double"
    ]
  }
}